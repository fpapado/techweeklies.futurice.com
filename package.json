{
  "name": "eleventy-base-blog",
  "version": "4.0.2",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build": "NODE_ENV=production npm-run-all -s clean build:postcss build:js build:eleventy build:postprocess",
    "build:js": "rollup --config rollup.config.js",
    "build:eleventy": "eleventy",
    "build:postcss": "postcss css/*.css --dir _site/css/ --no-map",
    "build:postprocess": "NODE_DIR=. node postprocessing/process.js",
    "clean": "rimraf _site/",
    "dev": "NODE_ENV=development npm-run-all -p dev:eleventy dev:postcss dev:js",
    "dev:js": "rollup --config rollup.config.js --watch",
    "dev:eleventy": "eleventy --watch --serve",
    "dev:postcss": "postcss css/*.css --dir _site/css/ --no-map --watch",
    "prod": "npm-run-all -s build serve",
    "serve": "serve _site --listen 8080",
    "debug": "DEBUG=* npx eleventy"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.5.4",
    "@11ty/eleventy-plugin-rss": "^1.0.3",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.0",
    "autoprefixer": "^9.3.1",
    "cheerio": "^1.0.0-rc.2",
    "cssnano": "^4.1.7",
    "debug": "^4.1.0",
    "gzip-size": "^5.0.0",
    "invariant": "^2.2.4",
    "luxon": "^1.0.0",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^5.0.2",
    "metalsmith": "^2.3.0",
    "multimatch": "^2.1.0",
    "npm-run-all": "^4.1.3",
    "postcss": "^7.0.5",
    "postcss-cli": "^6.0.1",
    "postcss-import": "^12.0.1",
    "purgecss": "^1.1.0",
    "purgecss-from-html": "^1.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^3.0.0",
    "serve": "^10.0.2"
  },
  "dependencies": {
    "perfume.js": "^2.0.0",
    "tachyons": "^4.11.1"
  }
}
